{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "prettier/@typescript-eslint",
    "prettier/airbnb",
    "prettier/react"
  ],
  "plugins": ["react", "@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": "error",
